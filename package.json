{
  "name": "node-libcurl",
  "version": "0.6.3",
  "author": "Jonathan Cardoso <me@jonathancardoso.com>",
  "description": "Node bindings for libcurl. Based on the work from node-curl.",
  "keywords": [
    "node-curl",
    "curl",
    "libcurl",
    "node-libcurl"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JCMais/node-libcurl",
  "repository": {
    "type": "git",
    "url": "git://github.com/JCMais/node-libcurl.git"
  },
  "main": "./index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha test --reporter spec"
  },
  "binary": {
    "module_name": "node_libcurl",
    "module_path": "./lib/binding/",
    "host": "https://github.com/JCMais/node-libcurl/releases/download",
    "remote_path" : "./v{version}/"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "dependencies": {
    "bindings": "~1.2",
    "debug": "^2.2",
    "nan": "~2.0",
    "node-gyp": "~3.0",
    "node-pre-gyp": "~0.6.11",
    "npmlog" : "~1.2.1"
  },
  "devDependencies": {
    "body-parser": "^1.12",
    "cookie-parser": "^1.3",
    "express": "^4.12",
    "http-auth": "^2.2",
    "jaguarjs-jsdoc": "davidshimjs/jaguarjs-jsdoc",
    "jsdoc": "^3.3",
    "jsdoc-baseline": "hegemonic/baseline",
    "mocha": "^2.2",
    "multiparty": "^4.1",
    "octokat": "~0.4.12",
    "progress": "~1.1.8",
    "should": "^7.0"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 2.14"
  }
}
