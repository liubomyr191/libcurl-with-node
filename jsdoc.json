{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": ["lib"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "baseline": "./jsdoc-tmpl.config.json"
  },
  "opts": {
    "template": "./node_modules/jsdoc-baseline",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "package": "./package.json"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
